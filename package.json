{
  "name": "asa-sdk",
  "version": "0.0.1",
  "description": "This is a library for asa widget",
  "main": "dist/index.mjs",
  "module": "dist/index.cjs.js",
  "type": "module",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rollup --config",
    "lint": "eslint .",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.4.1",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "rollup": "^4.21.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
